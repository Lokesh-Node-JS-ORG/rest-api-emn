{
  "name": "rest-api-express-mongo",
  "version": "1.3.0",
  "description": "Rest Full API Built on top of Express and MongoDB with NodeJS",
  "main": "index.ts",
  "engines": {
    "node": ">=12.22.0 <15"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "eslint": "eslint src/**/*.ts --ext .ts",
    "eslint:fix": "eslint src/**/*.ts --fix",
    "prettier:fix": "prettier --config .prettierrc src/**/*.ts --write",
    "start": "nodemon index.ts",
    "start:server": "nodemon --ignore client index.ts",
    "postinstall": "cd client && yarn install",
    "prestart": "cd client && yarn build",
    "webpack-dev-server": "cd client && yarn start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lokeshg-fsd/rest-api-emn.git"
  },
  "keywords": [
    "nodejs",
    "mongodb",
    "express"
  ],
  "author": "Lokesh G",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lokeshg-fsd/rest-api-emn/issues"
  },
  "homepage": "https://github.com/lokeshg-fsd/rest-api-emn#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-jwt": "^7.7.5",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.3.6",
    "nodemon": "^2.0.16",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.17.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2"
  }
}
